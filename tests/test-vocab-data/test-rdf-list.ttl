@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dc11: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix test: <http://www.skosmos.skos/test-rdf-list/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix skosmos: <http://www.skosmos.skos/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

skosmos:hasRelatedConcept a rdf:Property ;
    rdfs:label "Has related concept"@en ;
    rdfs:comment "A property for testing RDF lists with related concepts" .

# Steps in a Software Development Lifecycle
test:requirements a skos:Concept ;
    skos:inScheme test:conceptscheme ;
    skos:broader test:sdlc-ordered,
        test:mixed ;
    skos:prefLabel "Requirements Gathering"@en .
test:design a skos:Concept ;
    skos:inScheme test:conceptscheme ;
    skos:broader test:sdlc-ordered,
        test:mixed ;
    skos:prefLabel "System Design"@en .
test:implementation a skos:Concept ;
    skos:inScheme test:conceptscheme ;
    skos:broader test:sdlc-ordered,
        test:mixed ;
    skos:prefLabel "Implementation"@en .
test:testing a skos:Concept ;
    skos:inScheme test:conceptscheme ;
    skos:broader test:sdlc-ordered,
        test:mixed ;
    skos:prefLabel "Testing"@en .
test:deployment a skos:Concept ;
    skos:inScheme test:conceptscheme ;
    skos:broader test:sdlc-ordered,
        test:mixed ;
    skos:prefLabel "Deployment"@en .
test:maintenance a skos:Concept ;
    skos:inScheme test:conceptscheme ;
    skos:broader test:sdlc-ordered,
        test:mixed ;
    skos:prefLabel "Maintenance"@en .

