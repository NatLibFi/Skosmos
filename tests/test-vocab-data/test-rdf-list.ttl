# Test concept with ordered SDLC phases (6 items - below limit of 10)
test:sdlc-ordered a skos:Concept ;
    skos:inScheme test:conceptscheme ;
    skosmos:hasRelatedConcept (
        test:requirements
        test:design
        test:implementation
        test:testing
        test:deployment
        test:maintenance
    ) ;
    skos:narrower test:requirements,
        test:design,
        test:implementation,
        test:testing,
        test:deployment,
        test:maintenance ;
    skos:prefLabel "Software Lifecycle (Ordered)"@en ;
    skos:definition "A concept demonstrating ordered RDF list below truncation limit"@en ;
    skos:topConceptOf test:conceptscheme .

# Test concept with unordered programming languages (12 items - above limit of 10)
test:languages-unordered a skos:Concept ;
    skos:inScheme test:conceptscheme ;
    skosmos:hasRelatedConcept 
        test:python,
        test:java,
        test:javascript,
        test:c,
        test:csharp,
        test:ada,
        test:ruby,
        test:php,
        test:cpp,
        test:go,
        test:rust,
        test:objectivec,
        test:typescript,
        test:swift,
        test:r,
        test:kotlin,
        test:scala ;
    skos:narrower test:python,
        test:java,
        test:javascript,
        test:c,
        test:csharp,
        test:ada,
        test:ruby,
        test:php,
        test:cpp,
        test:go,
        test:rust,
        test:objectivec,
        test:typescript,
        test:swift,
        test:r,
        test:kotlin,
        test:scala ;
    skos:prefLabel "Programming Languages (Unordered)"@en ;
    skos:definition "A concept demonstrating unordered property values above truncation limit"@en ;
    skos:topConceptOf test:conceptscheme .

# Test concept with ordered programming languages (12 items - above limit of 10, TRUNCATED)
test:languages-ordered a skos:Concept ;
    skos:inScheme test:conceptscheme ;
    skosmos:hasRelatedConcept (
        test:python
        test:java
        test:javascript
        test:c
        test:csharp
        test:ada
        test:ruby
        test:php
        test:cpp
        test:go
        test:rust
        test:objectivec
        test:typescript
        test:swift
        test:r
        test:kotlin
        test:scala
    ) ;
    skos:narrower test:python,
        test:java,
        test:javascript,
        test:c,
        test:csharp,
        test:ada,
        test:ruby,
        test:php,
        test:cpp,
        test:go,
        test:rust,
        test:objectivec,
        test:typescript,
        test:swift,
        test:r,
        test:kotlin,
        test:scala;
    skos:prefLabel "Programming Languages (Ordered, Truncated)"@en ;
    skos:definition "A concept demonstrating ordered RDF list exceeding truncation limit"@en ;
    skos:topConceptOf test:conceptscheme .

# Test concept with mixed: some unordered + one ordered list
test:mixed a skos:Concept ;
    skos:inScheme test:conceptscheme ;
    skosmos:hasRelatedConcept 
        test:python,
        test:java,
        test:javascript,
        (
            test:requirements
            test:design
            test:implementation
            test:testing
            test:deployment
            test:maintenance
        ),
        test:kotlin ;
    skos:narrower test:python,
        test:java,
        test:javascript,
        test:requirements,
        test:design,
        test:implementation,
        test:testing,
        test:deployment,
        test:maintenance,
        test:kotlin;
    skos:prefLabel "Mixed (Unordered + Ordered)"@en ;
    skos:definition "A concept with both regular values and an ordered RDF list"@en ;
    skos:topConceptOf test:conceptscheme .

test:conceptscheme a skos:ConceptScheme ;
    rdfs:label "RDF List Test Vocabulary"@en ;
    dct:modified "2025-12-25T10:00:00+00:00"^^xsd:dateTime ;
    owl:versionInfo "Test vocabulary for RDF list functionality"^^xsd:string ;
    skos:hasTopConcept 
        test:sdlc-ordered,
        test:languages-unordered,
        test:languages-ordered,
        test:mixed .

