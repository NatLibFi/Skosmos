{
  "name": "nat-lib-fi/skosmos",
  "description": "Thesaurus and controlled vocabulary browser using SKOS and SPARQL",
  "type": "project",
  "license": "MIT",
  "repositories": { 
    "urijs-unofficial": {
      "type": "package",
      "package": {
        "name": "medialize/uri.js",
        "version": "1.18.2",
        "source": {
          "url": "https://github.com/medialize/URI.js.git",
          "type": "git",
          "reference": "v1.18.2"
        }
      }
    },
    "bootstrap-multiselect": {
      "type": "package",
      "package": {
        "name": "davidstutz/bootstrap-multiselect",
        "version": "0.9.13",
        "source": {
          "url": "https://github.com/davidstutz/bootstrap-multiselect.git",
          "type": "git",
          "reference": "v0.9.13"
        }
      }
    } ,
	 "netage-easyrdf": {
        "type":"package",
        "package": {
          "name": "netage/easyrdf",
          "version":"0.9.1",
          "source": {
              "url": "https://github.com/netage/easyrdf.git",
              "type": "git",
              "reference":"0.9.x"
            },
            "autoload": {
                    "classmap": ["lib"]
                }
        }
    }
  },
  "require": {
    "components/jquery": "1.12.*",
    "components/jqueryui": "1.12.*",
    "components/handlebars.js": "v1.3.0",
    "davidstutz/bootstrap-multiselect": "v0.9.13",
    "netage/easyrdf": "0.9.*",
    "twig/twig": "1.22.*",
    "twig/extensions": "1.3.*",
    "twitter/bootstrap": "3.2.*",
    "twitter/typeahead.js": "v0.10.5",
    "willdurand/negotiation": "1.5.*",
    "vakata/jstree": "3.3.*",
    "punic/punic": "1.6.0",
    "ml/json-ld": "1.*",
    "medialize/uri.js": "1.18.2",
    "ext-gettext": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8 - 5.1",
    "umpirsky/twig-gettext-extractor": "1.1.*",
    "phpdocumentor/phpdocumentor": "2.*",
    "codeclimate/php-test-reporter": "0.3.2"
  },
  "autoload": {
    "classmap": ["controller/", "model/", "model/sparql/"]
  }
}
